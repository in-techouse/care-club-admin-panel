<% extend ('../../layout')%>
<div class="app-page-title">
    <div class="page-title-wrapper">
        <div class="page-title-heading">
            <div class="page-title-icon">
                <i class="metismenu-icon fa fa-money-bill fa-w-6 icon-gradient bg-mean-fruit"></i>
            </div>
            <div class="row">
                Listing All Donations
            </div>
        </div>
    </div>
</div>
<div class="main-card mb-3 card">
    <input type="hidden" id="ngoId" value="<%= ngo.ngoId %>" />
    <div class="card-body">
        <h5 class="card-title">ALL DONATIONS</h5>
        <table class="mb-0 table table-bordered" id="myDonations">
            <thead>
                <tr>
                    <th>User Name</th>
                    <th>Amount</th>
                    <th>Date</th>
                    <th>Payment Method</th>
                    <th>Account Holder Name</th>
                    <th>Account Number</th>
                </tr>
            </thead>
        </table>
    </div>
</div>
<script src="/public/js/ngo/myDonations.js"></script>